
<div class="report-link">
    <p><%= link_to("新規記録","/posts/new")%></p>
    <p><%= link_to("全記録一覧","/posts/index")%></p>
</div>
<div class="report-title">
    <h1 class="all-title">REPORTS</h1>
    <p class="description">勉強した内容をまとめました</p>       
</div>
<%# ---- メインレポート画面 ---- %>

<div class="each-contents-1">
            <div class="reports">
                <p class="year-number-1">↓ 2020年10月~ ↓</p>
                <ul>
                    <% @months_2020.each do |month| %>
                    <li class="report-1">
                        <img id="flower" src="/hanawaku.png" alt="">
                        <p id="october"><%= link_to(month,"posts/months?year=#{+@year_2020.to_s}&month="+month.to_s)%></p>
                    </li>
                    <% end %>
                </ul>
                <p class="year-number-2">↓ 2021年1月~ ↓</p>
                <ul>
                    <% @months_2021.each do |month| %>
                    <li class="report-1">
                        <img id="flower" src="/hanawaku.png" alt="">
                        <p id="october"><%= link_to(month,"posts/months?year=#{+@year_2021.to_s}&month="+month.to_s)%></p>
                    </li>
                    <% end %>
                </ul>
            </div>
        </div>